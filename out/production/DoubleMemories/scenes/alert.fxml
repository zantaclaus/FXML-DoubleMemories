<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AlertController">
    <children>
        <Label layoutX="127.0" layoutY="25.0" text="EXIT">
            <font>
                <Font name="Aldrich Regular" size="65.0" />
            </font>
        </Label>
        <Button fx:id="closeBtn" layoutX="223.0" layoutY="163.0" mnemonicParsing="false" onAction="#noClick" prefHeight="58.0" prefWidth="153.0" text="NO">
            <font>
                <Font size="28.0" />
            </font>
        </Button>
        <Button layoutX="24.0" layoutY="163.0" mnemonicParsing="false" onAction="#yesClick" prefHeight="58.0" prefWidth="153.0" text="YES">
            <font>
                <Font size="28.0" />
            </font>
        </Button>
    </children>
</AnchorPane>

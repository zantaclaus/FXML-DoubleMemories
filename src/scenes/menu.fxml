<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MenuController">
    <children>
        <Label layoutX="257.0" layoutY="194.0" text="!DOUBLE MEMORIES">
            <font>
                <Font name="Aldrich Regular" size="67.0" />
            </font>
        </Label>
        <Button fx:id="btnNoob" layoutX="510.0" layoutY="346.0" mnemonicParsing="false" onAction="#noobMode" prefHeight="80.0" prefWidth="180.0" text="NOOB">
            <font>
                <Font name="Aldrich Regular" size="36.0" />
            </font>
        </Button>
        <Button fx:id="btnPro" layoutX="510.0" layoutY="470.0" mnemonicParsing="false" onAction="#proMode" prefHeight="80.0" prefWidth="180.0" text="PRO">
            <font>
                <Font name="Aldrich Regular" size="36.0" />
            </font>
        </Button>
        <Button fx:id="btnExit" layoutX="510.0" layoutY="601.0" mnemonicParsing="false" onAction="#exitProgram" prefHeight="80.0" prefWidth="180.0" text="EXIT">
            <font>
                <Font name="Aldrich Regular" size="36.0" />
            </font>
        </Button>
    </children>
</AnchorPane>

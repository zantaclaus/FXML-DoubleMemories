<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AlertController">
   <children>
      <ImageView fitHeight="250.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/alert.png" />
         </image>
      </ImageView>
      <Button fx:id="yesBtn" layoutX="67.0" layoutY="143.0" mnemonicParsing="false" onAction="#yesClick" opacity="0.0" prefHeight="57.0" prefWidth="87.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="noBtn" layoutX="248.0" layoutY="143.0" mnemonicParsing="false" onAction="#noClick" opacity="0.0" prefHeight="57.0" prefWidth="87.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
